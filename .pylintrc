[MESSAGES CONTROL]
disable=
    missing-docstring,
    too-few-public-methods,
    import-error,
    no-member,
    unused-argument,
    missing-type-doc,
    missing-return-type-doc,
    missing-param-doc,
    invalid-name,
    line-too-long,
    fixme,
    broad-except,
    wrong-import-position

[BASIC]
good-names=db,id,i,j,k,ex,Run,_,e

[FORMAT]
max-line-length=120